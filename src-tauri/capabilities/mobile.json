{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "enables the default permissions for mobile platforms",
  "windows": ["main"],
  "platforms": ["iOS", "android"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:webview:default",
    "core:app:default",
    "core:resources:default",
    "core:image:default"
  ]
}
