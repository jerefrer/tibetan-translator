{
  "productName": "TibetanTranslator",
  "version": "1.4.2",
  "identifier": "com.jerefrer.TibetanTranslator",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8080",
    "beforeDevCommand": "pnpm run serve",
    "beforeBuildCommand": "pnpm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Tibetan Translator",
        "width": 1280,
        "height": 1024,
        "resizable": true,
        "fullscreen": false,
        "dragDropEnabled": false
      },
      {
        "label": "global-lookup-popup",
        "title": "Tibetan Lookup",
        "url": "popup.html",
        "width": 650,
        "height": 500,
        "minWidth": 450,
        "minHeight": 300,
        "resizable": true,
        "minimizable": false,
        "maximizable": false,
        "alwaysOnTop": true,
        "decorations": false,
        "skipTaskbar": true,
        "shadow": true,
        "visible": false,
        "visibleOnAllWorkspaces": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../public/packs/core.sqlite": "core.sqlite",
      "../public/packs/pack-manifest.json": "pack-manifest.json"
    },
    "shortDescription": "The best Tibetan dictionaries in a pleasant interface for all devices",
    "longDescription": "An attempt at providing access to the best Tibetan dictionaries in a pleasant interface for all devices.",
    "iOS": {
      "minimumSystemVersion": "13.0"
    },
    "android": {
      "minSdkVersion": 24
    }
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/jerefrer/tibetan-translator/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZCRDhGRDE1NUZDN0Y0QjMKUldTejlNZGZGZjNZK3hTV1o3NWxpUnFSWnVqdmViSWlkTWZOYUR1M1JjcEt4ZnJtQzBVcm5kOE4K"
    }
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}